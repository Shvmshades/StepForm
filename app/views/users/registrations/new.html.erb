<center>
<h2>Sign up step1</h2>
<hr><hr>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>

  <div class="row">
    <div class="col-lg-6 m-auto">
  <div class="form-floating mb-3">
  <%= f.email_field :email, autofocus: true, class: "form-control", autocomplete: "email" %>
  <label for="floatingInput"><%= f.label :email %></label>
</div>
<div class="form-floating mb-3">
  <!-- <input type="password" class="form-control" id="floatingPassword" placeholder="Password"> -->
   <%= f.password_field :password,class:"form-control", autocomplete: "new-password"%>
  <label for="floatingPassword"><%= f.label :password %></label>
</div>

<div class="form-floating">
  <!-- <input type="password" class="form-control" id="floatingPassword" placeholder="Password"> -->
   <%= f.password_field :password_confirmation,class:"form-control", autocomplete: "new-password" %>
  <label for="floatingPassword"><%= f.label :password_confirmation %></label>
</div>

</div>

  <br>
  <div class="actions my-2">
    <%= f.submit "NEXT",class: "btn btn-info" %>
  </div>
<% end %>
</div>
</center>
<%#= render "users/shared/links" %>
