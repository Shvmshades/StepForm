<%= form_for @user, url: wizard_path do |f|%>


<div class="row">
    <div class="col-lg-6 m-auto">
  <div class="form-floating mb-3">
  <%= f.text_field :name, autofocus: true, class: "form-control", autocomplete: "name" %>
  <label for="floatingInput"><%= f.label :name %></label>
</div>


<div class="form-floating mb-3">
  <%= f.text_field :father_name, autofocus: true, class: "form-control", autocomplete: "father_name" %>
  <label for="floatingInput"><%= f.label :father_name %></label>
</div>
<div class="form-floating mb-3">
  <%= f.text_field :DOB, autofocus: true, class: "form-control datepicker", as: :date_field %>
  <label for="floatingInput"><%= f.label :DOB %></label>
</div>
<div class="actions my-3">
<%= f.submit "NEXT", class: "btn btn-primary"%>
</div>
<%end%>
</div>
</div>
</center>
  <script type="text/javascript">
	$(document).ready(function(){
		$('.datepicker').datepicker();
	})
</script>
